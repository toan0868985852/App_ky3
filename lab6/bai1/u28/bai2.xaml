<Page
    x:Class="u28.bai2"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:u28"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

    <Page.Resources>
        <SolidColorBrush x:Key="AccentBrush" Color="{ThemeResource SystemAccentColor}"/>
    </Page.Resources>

    <Grid>
        <StackPanel>
            <Rectangle Width="200" Height="200" Fill="{StaticResource SystemAccentColor}"/>
            <Rectangle Width="100" Height="100" Fill="{StaticResource SystemColorWindowColor}"/>
            <TextBlock Text="My App Title" Style="{StaticResource CaptionTextBlockStyle}"/>
        </StackPanel>
    </Grid>
</Page>
